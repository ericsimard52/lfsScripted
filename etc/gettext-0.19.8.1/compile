mPush gettext-tools
processCmd "make -C gnulib-lib"
[ $? -gt 0 ] && log "PKG|ERROR|Error with gnulib-lib" t && mPop && return 1

processCmd "make -C intl pluralx.c"
[ $? -gt 0 ] && log "PKG|ERROR|Error with intl pluralx.c" t && mPop && return 1

processCmd "make -C src msgfmt"
[ $? -gt 0 ] && log "PKG|ERROR|Error with src msgfmt" t && mPop && return 1

processCmd "make -C src msgmerge"
[ $? -gt 0 ] && log "PKG|ERROR|Error with src msgmerge" t && mPop && return 1

processCmd "make -C src xgettext"
[ $? -gt 0 ] && log "PKG|ERROR|Error with src xgettext" t && mPop && return 1

mPop
return 0
