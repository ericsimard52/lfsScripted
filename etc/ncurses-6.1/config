log "PKG|INFO|Ensure gawk is found during compilation"
processCmd "sed -i s/mawk// configure"
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with sed." t
    return 1
fi

log "PKG|INFO|Configuring." t
processCmd "./configure --prefix=/tools --with-shared --without-debug --without-ada --enable-widec --enable-overwrite"
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with configure." t
    return 1
fi
return 0
