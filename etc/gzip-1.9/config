log "PKG|INFO|Make some fixes required by glibc" t
sed -i 's/IO_ftrylockfile/IO_EOF_SEEN/' lib/*.c
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with sed." t
    return 1
fi
echo "#define _IO_IN_BACKUP 0x100" >> lib/stdio-impl.h
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with echo." t
    return 1
fi
processCmd "./configure --prefix=/tools"
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with configure." t
    return 1
fi
return 0
