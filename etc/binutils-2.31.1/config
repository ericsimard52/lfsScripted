function configPass1 {
    processCmd "../configure --prefix=/usr --enable-gold --enable-ld=default --enable-plugins --enable-shared --disable-werror --enable-64-bit-bfd --with-system-zlib"
    if [[ $? > 0 ]]; then
        log "PKG|ERROR|Error in configPass1.";
        return 1
    fi
}


promptUser "Which pass [1]"
read pass


case $pass in
    1)
        configPass1
        ;;
    *)
        log "{GEN,PKG,ERR}|ERROR|Choosed invalid pass." t t
        return 1
        ;;
esac


