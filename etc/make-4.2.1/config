log "PKG|INFO|Implement work around an error caused by glibc" t
processCmd "sed -i '211,217 d; 219,229 d; 232 d' glob/glob.c"
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with sed." t
    return 1
fi
processCmd "./configure --prefix=/tools --without-guile"
if [[ $? > 0 ]]; then
    log "PKG|ERROR|Error with configure." t
    return 1
fi
return 0
