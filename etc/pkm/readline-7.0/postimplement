eval "mv -v /usr/lib/lib{readline,history}.so.* /lib"
if [[ $? > 0 ]]; then
    return 1
fi
eval "chmod -v u+w /lib/lib{readline,history}.so.*"
if [[ $? > 0 ]]; then
    return 1
fi
eval "ln -sfv ../../lib/$(readlink /usr/lib/libreadline.so) /usr/lib/libreadline.so"
if [[ $? > 0 ]]; then
    return 1
fi
eval "ln -sfv ../../lib/$(readlink /usr/lib/libhistory.so ) /usr/lib/libhistory.so"
if [[ $? > 0 ]]; then
    return 1
fi
