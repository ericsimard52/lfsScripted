eval "make INSTALL_HDR_PATH=$FAKEROOT/$sdn headers_install"
if [[ $? > 0 ]]; then
    exit 1
fi

eval "find $FAKEROOT/$sdn/include \( -name .install -o -name ..install.cmd \) -delete"
if [[ $? > 0 ]]; then
    exit 2
fi
echo "cp -rv $FAKEROOT/$sdn/include/* /usr/include" |tee -a $ld/${lf[9]} 2>> $ld/$lf
if [[ $? > 0 ]]; then
    exit 3
fi


