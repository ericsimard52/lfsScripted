function installPass1 {
    make install
    if [[ $? > 0 ]]; then
        return 1
    fi
}
function installPass2 {
    make install
    if [[ $? > 0 ]]; then
        return 2
    fi
}

promptUser "Which Pass? 1, 2"
read u
case $u in
    1)
        installPass1
        ;;
    2)
        installPass2
        ;;
    *)
        log "ERROR: Unknown choice."
        ;;
esac
