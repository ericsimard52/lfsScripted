function pass1 {
    eval "make"
    if [[ $? > 0 ]]; then
        return 1
    fi
}

function makeStdc {
    eval "make"
    if [[ $? > 0 ]]; then
        return 1
    fi
}


promptUser "Which pass are we running? 1, [L]ibstdc++"
read u
case $u in
    1)
        pass1
        ;;
    [Ll])
        makeStdc
        ;;
esac
