function pass2 {
    ln -sv gcc /tools/bin/cc
    if [[ $? > 0 ]]; then
        return 1
    fi
}
function pass1 {
    log "INFO: Nothing to do." t
}

function installStdc {
    log "INFO: Nothing to do." t
}

promptUser "Which pass are we running? 1, 2, [L]ibstdc++"
read u
case $u in
    1)
        pass1
        ;;
    2)
        pass2
        ;;
    [Ll])
        installStdc
        ;;
esac
