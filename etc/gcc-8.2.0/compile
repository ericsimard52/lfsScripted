
function compilePass1 {
    processCmd "make"
    if [[ $? > 0 ]]; then
        log "PKG|ERROR|Error compiling pass 1." t
        return 1
    fi
}
promptUser "Which pass [1]"
read pass
case $pass in
    1)
        compilePass1
        if [[ $? > 0 ]]; then
            log "PKG|ERROR|Error withing compile source scripts." t
            return 1
        fi

        ;;
    *)
        log "PKG|ERROR|Invalid option."
        return 1
        ;;
esac
